/*点击显示全部信息 */

.delta {
  width: 20rpx;
  height: 20rpx;
  transform: rotate(45deg);
  background-color: #f9f9f9;
  border-bottom: 1px solid #ddd;
  border-right: 1px solid #ddd;
  margin: 0 auto;
  margin-bottom: -20rpx;
}

.showInfo {
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 80rpx;
  border: 1px solid #ddd;
  border-radius: 10rpx;
  padding: 10rpx;
  background-color: #f9f9f9;
  z-index: 1000;
}

