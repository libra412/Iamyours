/* 公共底部工具栏 */

.g-bottom-bar {
  position: fixed;
  bottom: 0;
  width: 100%;
}

.g-bottom-btn {
  height: 45px;
  line-height: 45px;
  background-color: #e94b45;
  border-radius: 0;
  color: #fff;
  text-align: center;
}

/* 公共弹窗 */

.g-pop-wrap {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  text-align: center;
  background-color: rgba(0, 0, 0, 0.6);
}

.g-pop-content {
  position: relative;
  display: inline-block;
  width: 80%;
  padding-top: 36px;
  background-color: #fff;
  border-radius: 8px;
  vertical-align: middle;
  overflow: hidden;
}

.g-pop-close {
  position: absolute;
  top: 8px;
  right: 8px;
  width: 26px;
  height: 26px;
  padding: 5px;
}

.g-pop-confirm {
  width: 100%;
  height: 50px;
  line-height: 50px;
  text-align: center;
  font-size: 18px;
  color: #fff;
  background-color: #e94b45;
  border-radius: 0;
}

